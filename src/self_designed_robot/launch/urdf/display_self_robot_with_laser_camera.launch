<launch>

    <param name="robot_description" textfile="$(find self_designed_robot)/urdf/my_robot_with_laser_camera.urdf" />
    <param name="use_gui" value="true" />

    <node name="joint_state_publisher_gio" pkg="joint_state_publisher_gui" type="joint_state_publisher_gui" />
    <node name="robot_state_publisher" pkg="robot_state_publisher" type="state_publisher" />
    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find self_designed_robot)/config/my_robot_with_laser_camera.rviz" required="true" />

</launch>
